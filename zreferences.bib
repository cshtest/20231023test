@article{campbell_efficiency_1985,
	title = {{EFFICIENCY} {LIMITS} {FOR} {SILICON} {CONCENTRATOR} {SOLAR} {CELLS}.},
	issn = {0160-8371},
	abstract = {The intrinsic limits on the energy conversion efficiency of silicon solar cells when they are used under concentrated sunlight are discussed. It is shown that Auger recombination processes are even more important under concentrated sunlight than under nonconcentrated sunlight. However, light trapping can be far more effective under concentrated light due to the better defined direction of the incident light. As a result of these effects, it is found that the limiting efficiency lies in the 36-37\% range regardless of concentration ratio compared to the limiting value of 29. 8\% for a nonconcentrating cell with isotropic response.},
	journal = {Conference Record of the IEEE Photovoltaic Specialists Conference},
	author = {Campbell, P and Green, MA},
