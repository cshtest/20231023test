@inproceedings{theden_fully_1990,
	title = {Fully integrated {Schottky} array. {A} new generation of metal silicide infrared detectors},
	volume = {1308},
	isbn = {0-8194-0359-8},
	abstract = {We propose a new generation of metal-silicide Schottky infrared detector arrays. These arrays have high detector fill factors, small pixel sizes, and a short manufacturing sequence which should lead to high processing yields of large detector arrays. Our 'Fully Integrated Schottky Array' (FISA) uses metal silicide gates, which lie directly on silicon, for both infrared detection and charge transfer. The detector array is designed as a frame transfer device. An array of infrared sensitive Schottky CCD gates is connected to an array of non-sensitive Schottky CCD gates for frame storage before reading out line by line through a Schottky gate read-out CCD. The development of the FISA involves new ways of sub-micron patterning of the gaps between the metal Schottky gates using conventional photolithographic techniques. Low Schottky barrier height silicides are used for infrared detection, metal silicides having high Schottky barrier heights are used for the non-infrared-sensitive storage CCDs and the read-out CCDs. Since the Schottky CCDs use no gate oxides, the charge handling capabilities are higher than in conventional CCDs. In addition, the device is more radiation resistant, and has inbuilt anti-blooming features.},
	booktitle = {Proceedings of {SPIE} - {The} {International} {Society} for {Optical} {Engineering}},
