@article{laponsky_evision_1974,
	title = {℡{EVISION} {CAMERA} {TUBES} {AND} {DIRECT} {VIEW} {INTENSIFIERS} {FOR} {LOW} {LIGHT} {LEVEL} {IMAGING} {WITH} {ULTRAVIOLET}, {VISIBLE}, {AND} {INFRA}-{RED} {RADIATION}.},
	volume = {40},
	issn = {0030-4026},
	abstract = {Television camera tubes and direct view image intensifiers have shown a remarkable ability to extend the range of human vision to very low light levels and into spectral regions where the eye is not sensitive. A variety of devices currently in existance permit both subjective viewing and quantitative analysis of images inaccessible to the unaided human eye. In addition to their normal use to provide real time images, for immediate viewing by an observer, both direct view devices and television cameras can be used to obtain time exposures of stationary scenes. In these applications of light flux as low as one photon per picture element per minute can be integrated over an extended period to form a useful image. Starting with the fundamental physical limitations to vision this paper discusses electronic low light imaging in the electromagnetic spectrum from the vacuum ultraviolet to the near infrared. The detection of weak X-ray, gamma-ray, XUV, beta particle, and neutron images with the aid of suitable energy conversion screens is also described. A variety of industrial and scientific applications are used as illustrative examples of the mode of operation and performance characteristics of the more important practical sensors.},
	number = {4},
	journal = {Optik (Jena)},
	author = {Laponsky, AB and Doughty, DD and Green, M and Goetze, GW},
	month = jan,
	year = {1974},
	pages = {381--399},
}

